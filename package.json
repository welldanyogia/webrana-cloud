  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "npx prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "npx prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "npx nx run-many -t typecheck",
    "db:generate": "prisma generate --schema=apps/auth-service/prisma/schema.prisma && prisma generate --schema=apps/order-service/prisma/schema.prisma && prisma generate --schema=apps/billing-service/prisma/schema.prisma && prisma generate --schema=apps/catalog-service/prisma/schema.prisma && prisma generate --schema=apps/notification-service/prisma/schema.prisma",
    "db:migrate:dev": "npm run db:migrate:dev:auth && npm run db:migrate:dev:order && npm run db:migrate:dev:billing && npm run db:migrate:dev:catalog && npm run db:migrate:dev:notification",
    "db:migrate:deploy": "npm run db:migrate:deploy:auth && npm run db:migrate:deploy:order && npm run db:migrate:deploy:billing && npm run db:migrate:deploy:catalog && npm run db:migrate:deploy:notification",
    "db:migrate:dev:auth": "prisma migrate dev --schema=apps/auth-service/prisma/schema.prisma",
    "db:migrate:deploy:auth": "prisma migrate deploy --schema=apps/auth-service/prisma/schema.prisma",
    "db:migrate:dev:order": "prisma migrate dev --schema=apps/order-service/prisma/schema.prisma",
    "db:migrate:deploy:order": "prisma migrate deploy --schema=apps/order-service/prisma/schema.prisma",
    "db:migrate:dev:billing": "prisma migrate dev --schema=apps/billing-service/prisma/schema.prisma",
    "db:migrate:deploy:billing": "prisma migrate deploy --schema=apps/billing-service/prisma/schema.prisma",
    "db:migrate:dev:catalog": "prisma migrate dev --schema=apps/catalog-service/prisma/schema.prisma",
    "db:migrate:deploy:catalog": "prisma migrate deploy --schema=apps/catalog-service/prisma/schema.prisma",
    "db:migrate:dev:notification": "prisma migrate dev --schema=apps/notification-service/prisma/schema.prisma",
    "db:migrate:deploy:notification": "prisma migrate deploy --schema=apps/notification-service/prisma/schema.prisma",
    "build": "npx nx run-many -t build",
    "test": "npx nx run-many -t test"
  },
